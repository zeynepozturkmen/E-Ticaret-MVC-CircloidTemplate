<body>
    <div class="container-fluid">
        <div id="body-container">
            <div class="standalone-page registration-page">
                <div class="standalone-page-logo">
                    <a href="/Content/index.html">
                        <img src="/Content/assets/images/required/logo-default.png" width="156" height="44" alt="Logo" />
                    </a>
                </div>
                <div class="standalone-page-top-border">
                    <span class="border-block bg-color-green"></span>
                    <span class="border-block bg-color-orange"></span>
                    <span class="border-block bg-color-yellow"></span>
                    <span class="border-block bg-color-blue"></span>
                    <span class="border-block bg-color-red"></span>
                    <span class="border-block bg-color-lime"></span>
                    <span class="border-block bg-color-purple"></span>
                </div>
                <div class="standalone-page-block">
                    <div class="row">
                        <div class="col-xs-12">
                            <h2 class="heading">
                                <span aria-hidden="true" class="icon icon-user"></span>
                                <span class="main-text">
                                    Yeni Hesap Oluştur
                                </span>
                            </h2>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">

                            <form role="form" class="registration-form form-horizontal" method="post" action="/Member/CreateNewAccount">
                                <div class="form-group">
                                    <label for="inputUsername" class="col-sm-3 control-label">Kullanıcı Adı:</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputUsername" placeholder="Kullanıcı Adını Giriniz.." type="text" name="UserName">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputEmail" class="col-sm-3 control-label">Email</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="inputEmail" placeholder="Emailinizi Giriniz." type="email" name="Email">
                                    </div>
                                </div>



                                <div class="form-group">
                                    <label for="inputPassword" class="col-sm-3 control-label" id="txtPassword">Şifre:</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" placeholder="Şifrenizi Giriniz." type="password" name="Password" id="inputPassword" onblur="validate()">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputPassword" class="col-sm-3 control-label" id="txtConfirmPassword">Şifre tekrarı:</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" placeholder="Şifrenizi tekrar Giriniz." type="password" name="ConfirmPassword" onblur="validate()" id="inputConfirmPassword">
                                    </div>
                                </div>


                                @*<script>
                                    function validate() {
                                        var x = document.getElementById("inputPassword");
                                        var y = document.getElementById("inputConfirmPassword");
                                        var span = getElementById("hata");
                                        if (x.value == y.value) {
                                            span.innerHTML = "aynı";
                                        }
                                        else {
                                            span.innerHTML = "farklı";
                                        }
                                    }
                                </script>*@


                                <div class="form-group">
                                    <label for="inputPassword" class="col-sm-3 control-label">Gizli Soru:</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="exampleInputPassword1" placeholder="Gizli Sorunuzu Giriniz." type="text" name="PasswordQuestion">
                                    </div>
                                </div>



                                <div class="form-group">
                                    <label for="inputPassword" class="col-sm-3 control-label">Gizli Cevap:</label>
                                    <div class="col-sm-9">
                                        <input autocomplete="off" class="form-control" id="exampleInputPassword1" placeholder="Gizli Cevabınızı Giriniz." type="text" name="PasswordAnswer">
                                    </div>
                                </div>



                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-9">
                                        <button type="submit" class="btn btn-success">Hesap Oluştur</button>
                                    </div>
                                </div>

                                <span id="hata">@ViewBag.createMessage</span>
                                <span class="text-success">@ViewBag.Message</span>



                            </form>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="change-section">
                                <h3 class="heading">Zaten bir hesabınız var mı??</h3>
                                <a href="/Member/MemberLogin" class="btn btn-default btn-block">Giriş Yap</a>
                            </div>
                        </div>
